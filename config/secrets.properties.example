# Application Secrets Configuration
# Copy this file to secrets.properties and fill in your actual values
# NEVER commit the actual secrets.properties file to version control!

# Database Configuration
db.driver=com.mysql.cj.jdbc.Driver
db.url=jdbc:mysql://localhost:3306/your_database_name
db.username=your_database_user
db.password=your_database_password

# Email Configuration
email.smtp.host=smtp.gmail.com
email.smtp.port=587
email.username=your_email@gmail.com
email.password=your_email_password

# Security Keys
app.secret.key=your_app_secret_key_here
jwt.secret=your_jwt_secret_key_here
session.secret=your_session_secret_key_here

# Third-party API Keys
openrouter.api.key=your_openrouter_api_key_here
google.api.key=your_google_api_key_here
facebook.api.key=your_facebook_api_key_here

# File Upload Security
upload.secret.key=your_upload_secret_key_here
encryption.key=your_encryption_key_here
